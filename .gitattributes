/.gitattributes     export-ignore
/.gitignore         export-ignore
/Gemfile            export-ignore
/Gemfile.lock       export-ignore
/gulpfile.js        export-ignore
/index.html         export-ignore
/robots.txt         export-ignore
/docs               export-ignore
